    var teveAlteracaoDoc = false;
    function rejeitarAceitarFoto(doc, aceitar, motivo, observacoes) {
        if (typeof motivo === 'undefined' || motivo === null) {
            motivo = "";
        }
        $.ajax({
            url: '<?php echo URLBASE_CLIENT . \Utils\Rotas::R_POSTACEITARREHEITARFOTOCLIENTE ?>',
            method: 'post',
            dataType: 'json',
            data: {
                doc:doc,
                aceitar:aceitar,
                idCliente: $("#cadastroClienteId").val(),
                motivo: motivo,
                observacoes:observacoes
            },
            success: function (json) {
                try {
                    if (json.sucesso) {
                        
                        if (doc === 'd') {
                            if (aceitar > 0) {
                                $("#cadastroClienteFotoDocumentoVerificada").show();
                                $(".btn-doc-ap").hide();
                            } else {
                                $("#cadastroClienteFotoDocumento").attr("src", "");
                                $("#cadastroClienteContainerFotoDocumento").hide();
                                $(".btn-doc").hide();
                            }
                            
                        } else if (doc === 'r') {
                            if (aceitar > 0) {
                                $("#cadastroClienteFotoResidenciaVerificada").show();
                                $(".btn-res-ap").hide();
                            } else {
                                $("#cadastroClienteFotoResidencia").attr("src", "");
                                $("#cadastroClienteContainerFotoResidencia").hide();
                                $(".btn-res").hide();
                            }
                            
                        } else if (doc === 'c') {
                            if (aceitar > 0) {
                                $("#cadastroClienteFotoClienteVerificada").show();
                                $(".btn-selfie-ap").hide();
                            } else {
                                $("#cadastroClienteFotoCliente").attr("src", "");
                                $("#cadastroClienteContainerFotoCliente").hide();
                                $(".btn-selfie").hide();
                            }
                            
                        } else if (doc === 'o') {
                            if (aceitar > 0) {
                                $("#cadastroClienteFotoOutroDocumentoVerificada").show();
                                $(".btn-pj-ap").hide();
                            } else {
                                $("#cadastroClienteFotoOutroDocumento").attr("src", "");
                                $("#cadastroClienteContainerFotoOutroDocumento").hide();
                                $(".btn-pj").hide();
                            }
                            
                        }
                        
                        teveAlteracaoDoc = true;
                    } else {
                        showNotyAlert(json.mensagem, "e");
                    }
                } catch (e) {
                    showNotyAlert(e, "e");
                }
            }
        });
