    function pagarPromocao(cliente) {
        $("#btn-promocao-"+cliente).prop("disabled", true);
        $.ajax({
            url: '<?php echo URLBASE_CLIENT . \Utils\Rotas::R_CLIENTES_PROMOCAO_PAGAR ?>',
            method: 'post',
            dataType: 'json',
            data: {
                cliente: cliente
            },
            success: function (json) {
                try {
                    if (json.sucesso) {
                        showNotyAlert(json.mensagem, "s");
                        $("#btn-promocao-"+cliente).remove();
                    } else {
                        showNotyAlert(json.mensagem, "e");
                        $("#btn-promocao-"+cliente).prop("disabled", false);
                    }
                } catch (e) {
                        showNotyAlert(json.mensagem, "e");
                        $("#btn-promocao-"+cliente).prop("disabled", false);
                }
            }
        });
    }


    function listarClientes() {
        $("#listaClientes").html("<div class='col-lg-12 text-center'><img src='<?php echo IMAGES ?>loading.gif' /></div>");
        $.ajax({
            url: '<?php echo URLBASE_CLIENT . \Utils\Rotas::R_POSTLISTARCLIENTES ?>',
            method: 'post',
            dataType: 'json',
            data: {
                u: $("#idUsuario").val(),
                s: $("#status").val(),
                p: $("#pais").val(),
                f: $("#filtro").val(),
                d: $("#statusDocumentos").val(),
                l: $("#filtroPerfil").val(),
                o: $("#ordenacao").val(),
                di: $("#dataInicial").val(),
                df: $("#dataFinal").val(),
                td: $("#tipoData").val(),
                online: $("#status-online").val()
            },
            success: function (json) {
                try {
                    if (json.sucesso) {
                        $("#listaClientes").html(json.html);
                        $("#clientesTodos").html(json.cadastrados);
                        $("#clientesListando").html(json.total);
                        $("#clientesAguardando").html(json.aguardando);
                        $("#clientesAprovados").html(json.aprovados);
                        $("#clientesNegados").html(json.negados);
                    } else {
                        $("#listaClientes").html("<div class='col-lg-12 text-center'>" + json.mensagem + "</div>");
                        $("#clientesTodos").html("0");
                        $("#clientesListando").html("0");
                        $("#clientesAguardando").html("0");
                        $("#clientesAprovados").html("0");
                        $("#clientesNegados").html("0");
                    }
                } catch (e) {
                    $("#listaClientes").html("<div class='col-lg-12 text-center'>" + e + "</div>");
                    $("#clientesTodos").html("0");
                    $("#clientesListando").html("0");
                    $("#clientesAguardando").html("0");
                    $("#clientesAprovados").html("0");
                    $("#clientesNegados").html("0");
                }
            }
        });
    }

    function alterarStatusFranquia(idCliente) {
        $.ajax({
            url: '<?php echo URLBASE_CLIENT . Utils\Rotas::R_CLIENTES_FRANQUIA_STATUS?>',
            method: 'post',
            dataType: 'json',
            data: {
                idCliente: idCliente
            },
            success: function (json) {
                try {
                    if (json.sucesso) {
                    if(json.franquia > 0){
                        $("#btnHabilitarLicenca").removeClass("btn-primary");
                        $("#btnHabilitarLicenca").addClass("btn-danger");
                        $("#btnHabilitarLicenca").html("Bloquear Licença");
                    } else {
                        $("#btnHabilitarLicenca").removeClass("btn-danger");
                        $("#btnHabilitarLicenca").addClass("btn-primary");
                        $("#btnHabilitarLicenca").html("Habilitar Licença");
                        
                     }                          
                        listarClientes();
                    } else {
                        showNotyAlert(json.mensagem, "e");
                    }
                } catch (e) {
                    showNotyAlert(e, "e");
                }
            }
        });
    }

