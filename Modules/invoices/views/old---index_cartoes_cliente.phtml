    function listarPedidosCartoes() {
        $("#pedidosCartoes").html("<div class='col col-lg-12 text-center'><img src='<?php echo IMAGES ?>loading.gif' /></div>");
        $.ajax({
            url: '<?php echo URLBASE_CLIENT . Utils\Rotas::R_INVOICES_LISTAR ?>',
            method: 'post',
            dataType: 'json',
            data: {
                ativo: $("#ativo").val()
            },
            success: function (json) {
                try {
                    if (json.sucesso) {
                        $("#pedidosCartoes").html(json.html);

                        $("#pedidosPendentes").html(json.pedidosPendentes);
                        $("#pedidosPagos").html(json.pedidosPagos);
                        $("#pedidosCancelados").html(json.pedidosCancelados);
                        $("#cartoesAtivos").html(json.cartoesAtivos);
                        $("#cartoesInativos").html(json.cartoesInativos);
                        $("#cartoesCancelados").html(json.cartoesCancelados);
                    } else {
                        $("#pedidosCartoes").html("<div class='col col-lg-12 text-center'>"+json.mensagem+"</div>");

                        $("#pedidosPendentes").html("0");
                        $("#pedidosPagos").html("0");
                        $("#pedidosCancelados").html("0");
                        $("#cartoesAtivos").html("0");
                        $("#cartoesInativos").html("0");
                        $("#cartoesCancelados").html("0");
                    }
                } catch (e) {
                    $("#pedidosCartoes").html("<div class='col col-lg-12 text-center'>"+e+"</div>");
                    $("#pedidosPendentes").html("0");
                    $("#pedidosPagos").html("0");
                    $("#pedidosCancelados").html("0");
                    $("#cartoesAtivos").html("0");
                    $("#cartoesInativos").html("0");
                    $("#cartoesCancelados").html("0");
                }
            }
        });

    }

    function dialogNovoPedido() {
        $("#dialogNovaInvoice").modal();
    }



    function ativarCartao(idPedidoCartao) {

        $.ajax({
            url: '<?php echo URLBASE_CLIENT . Utils\Rotas::R_INVOICES_CARTAO_GET  ?>invoices/invoices/getDadosCartao',
            method: 'post',
            dataType: 'json',
            data: {
                idPedidoCartao: idPedidoCartao
            },
            success: function (json) {
                try {
                    if (json.sucesso) {
                        $("#modalAtivarCartao").modal();
                        $("#modalAtivarCartaoIdPedidoCartao").val(idPedidoCartao);
                        $("#modalAtivarCartaoNumeroCartao").val(json.pedido.numeroCartao);
                        $("#modalAtivarCartaoSenhaCartao").val(json.pedido.senhaCartao);
                        $("#modalAtivarCartaoValidadeCartao").val(json.pedido.validade);
                        $("#modalAtivarCartaoNomeTitular").val(json.pedido.nomeCartao);

                        if (json.pedido.ativo > 0) {
                            $("#modalAtivarCartaoSalvar").prop("disabled", true);
                        } else {
                            $("#modalAtivarCartaoSalvar").prop("disabled", false);
                        }
                    } else {
                        showNotyAlert(json.mensagem, "e");
                    }
                } catch (e) {
                    showNotyAlert(e, "e");
                }
            }
        });

    }
    
    
    function novoCartao() {
        $("#dialogNovaInvoiceFechar, #dialogNovaInvoiceSalvar").prop("disabled", true);
        $.ajax({
            url: '<?php echo URLBASE_CLIENT . Utils\Rotas::R_INVOICES_CARTAO_NOVO ?>',
            method: 'post',
            dataType: 'json',
            data: {},
            success: function (json) {
                try {
                    if (json.sucesso) {
                        if (json.redirecionar) {
                            window.open(json.url, '_NEW_CARD');
                        } else {
                            showNotyAlert(json.mensagem, "s");
                        }
                        
                        $("#dialogNovaInvoiceFechar").prop("disabled", false).trigger("click");
                    } else {
                        showNotyAlert(json.mensagem, "e");
                    }
                } catch (e) {
                    showNotyAlert(e, "e");
                }
                $("#dialogNovaInvoiceFechar, #dialogNovaInvoiceSalvar").prop("disabled", false);
            }
        });
    }
    
    function confirmarAtivacaoCartao() {
        $("#modalAtivarCartaoFechar, #modalAtivarCartaoSalvar").prop("disabled", true);
        $.ajax({
            url: '<?php echo URLBASE_CLIENT . Utils\Rotas::R_INVOICES_CARTAO_ATIVAR ?>',
            method: 'post',
            dataType: 'json',
            data: {
                idPedidoCartao: $("#modalAtivarCartaoIdPedidoCartao").val(),
                numeroCartao: $("#modalAtivarCartaoNumeroCartao").val(),
                //senhaCartao: $("#modalAtivarCartaoSenhaCartao").val(),
                confirmarCartao: $("#modalAtivarCartaoConfirmarCartao").val(),
                //confirmarSenha: $("#modalAtivarCartaoConfirmarSenha").val(),
                validade: $("#modalAtivarCartaoValidadeCartao").val(),
                nomeCartao: $("#modalAtivarCartaoNomeTitular").val()
            },
            success: function (json) {
                try {
                    if (json.sucesso) {
                        listarPedidosCartoes();
                        $("#modalAtivarCartaoFechar").prop("disabled", false).trigger("click");
                        showNotyAlert(json.mensagem, "s");
                    } else {
                        showNotyAlert(json.mensagem, "e");
                    }
                } catch (e) {
                    showNotyAlert(e, "e");
                }
                $("#modalAtivarCartaoFechar, #modalAtivarCartaoSalvar").prop("disabled", false);
            }
        });
    }
        
        
    <?php }  ?>

    function modalPin(idCartao) {
        $("#modalPin").modal();
        $("#modalPin").val("");
        $("#modalPinIdPedidoCartao").val(idCartao);
    }
    
    
    function modalSemPin() {
        $("#modalSemPin").modal();
    }
    
    function validarPin() {
        $("#modalPinFechar, #modalPinValidar").prop("disabled", true);
        $.ajax({
            url: '<?php echo URLBASE_CLIENT . Utils\Rotas::R_INVOICES_PIN_VALIDAR ?>',
            method: 'post',
            dataType: 'json',
            data: {
                pin: $("#modalPinSenha").val()
            },
            success: function (json) {
                try {
                    if (json.sucesso) {
                        $("#modalPinFechar").prop("disabled", false).trigger("click");
                        verSenha($("#modalPinIdPedidoCartao").val());
                    } else {
                        showNotyAlert(json.mensagem, "e");
                    }
                } catch (e) {
                    showNotyAlert(e, "e");
                }
                $("#modalPinFechar, #modalPinValidar").prop("disabled", false);
            }
        });
    }

    function verSenha(idCartao) {
        $.ajax({
            url: '<?php echo URLBASE_CLIENT . Utils\Rotas::R_INVOICES_SENHA_MOSTRAR ?>',
            method: 'post',
            dataType: 'json',
            data: {
                idCartao: idCartao
            },
            success: function (json) {
                try {
                    if (json.sucesso) {
                        $("#modalSenhaCartaoSenha").html(json.senha);
                        $("#modalSenhaCartao").modal();
                    } else {
                        showNotyAlert(json.mensagem, "e");
                    }
                } catch(e) {
                    showNotyAlert(e, "e");
                }
            }
        });
    }
</script>



