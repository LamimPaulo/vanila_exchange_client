                tipoConta: $("#cadastroContaTipoConta").val(),
                documento: $("#documentoConta").val()
            },
            success: function (json) {
                try {
                    if (json.sucesso) {
                        listarContasBancarias();
                        limparConta();
                        showNotyAlert(json.mensagem, "s");
                    } else {
