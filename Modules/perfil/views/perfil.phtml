    function meusLimites() {
        $.ajax({
            url: '<?php echo URLBASE_CLIENT . Utils\Rotas::R_MEUSDADOS_LIMITES ?>',
            method: 'post',
            dataType: 'json',
            success: function (json) {
                try {
                    if (json.sucesso) {
                        if (json.verificado) {
                            $("#panelBasica").removeClass("panel-primary");
                            $("#footerBasica").removeClass("panel-primary");
                            $("#footerBasica").attr("hidden", true);
                            $("#panelVerificada").addClass("panel-primary");
                            $("#footerVerificada").addClass("panel-primary");
                            $("#footerVerificada").attr("hidden", false);
                        } else {
                            $("#panelBasica").addClass("panel-primary");
                            $("#footerBasica").addClass("panel-primary");
                            $("#footerBasica").attr("hidden", false);
                            $("#panelVerificada").removeClass("panel-primary");
                            $("#footerVerificada").removeClass("panel-primary");
                            $("#panelVerificada").addClass("panel-default");
                            $("#footerVerificada").attr("hidden", true);
                        }
                        $("#panelVerificada").attr("hidden", false);
                        $("#panelBasica").attr("hidden", false);
                    }
                } catch (e) {

                }
            }
        });
    }


    function loadCheckBox() {
        var swObjs = Array.prototype.slice.call(document.querySelectorAll('.js-switch'));
        $("input[type=checkbox]").each(function (index, element) {
            if (!element.getAttribute('data-switchery')) {
                var init = new Switchery(element, {
                    color: '#1ab394', size: 'small', disabledOpacity: 0.3
                });

                swObjs[element.id] = init;
            }
        });
    }

    function salvarConta() {
        $("#cadastroContaFechar, #cadastroContaSalvar").prop("disabled", true);
