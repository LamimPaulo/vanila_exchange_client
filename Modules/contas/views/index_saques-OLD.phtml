    function confirmarTransferencia(){
        $("#infMoeda").hide();
        $("#token2fa").hide();
        $("#carregaToken").show();
        $("#carregaToken").html("<img src='<?php echo IMAGES ?>loading.gif' />");
        
        $.ajax({
                url: '<?php echo URLBASE_CLIENT . Utils\Rotas::R_BTC_AUTORIZACAO_TOKEN  ?>',
                method: 'post',
                dataType: 'json',
                data: {
                    enderecoBitcoin:  $("#endereco").val(),
                    valor: $("#valor").val(),
                    moeda: moeda
                },
                success: function (json) {
                    try {
                        if (json.sucesso) {
                            $("#enderecoConfirmar").html($("#endereco").val());
                            $("#volumeConfirmar").html($("#debitadoBtc").html());
                            $("#carregaToken").hide();
                            $("#infMoeda").hide();
                            $("#token2fa").show();
                            showNotyAlert(json.mensagem, "s");
                        } else {
                            $("#carregaToken").hide();
                            $("#token2fa").hide();
                            $("#infMoeda").show();
                            showNotyAlert(json.mensagem, "e");
                        }
                    } catch (e) {
                        showNotyAlert(e, "e");
                        $("#carregaToken").hide();
                        $("#token2fa").hide();
                        $("#infMoeda").show();
                    }
                    
                }
            });
    }
            
