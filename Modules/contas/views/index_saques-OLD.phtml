    function getSaldoDisponivel(){
        $.ajax({
            url: '<?php echo URLBASE_CLIENT . Utils\Rotas::R_REAIS_SAQUES_SALDOS ?>',
            method: 'post',
            dataType: 'json',
            data:{
                id: moeda
            },
