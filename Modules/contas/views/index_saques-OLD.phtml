        var tarifaTransf = <?php echo number_format($configuracao->taxaTransferenciaInternaReais, 2, ".", "") ?>;
        var valorMinimo = <?php echo number_format($valorMinReais, 2, ".", "") ?>;
        
        if($("#idContaBancaria").val() == 1){
